#include <iostream>
using namespace std;
int main() {
    int MRnumA,MRnumB;
   cout<<"Ingrese de cuanto x cuanto sera su matriz A: ";cin>>MRnumA;
    cout<<"Ingrese de cuanto x cuanto sera su matriz B: ";cin>>MRnumB;
      int MRMatrizA[MRnumA][MRnumA],MRMatrizB[MRnumB][MRnumB];
        int MRsumadeAB[MRnumA][MRnumB];
      cout<<"Ingrese los datos de su matriz A: ";
    for(int i=0;i<MRnumA;i++){
            for(int j=0;j<MRnumA;j++){
                cin>>MRMatrizA[i][j];
            }
    }
          cout<<"Ingrese los datos de su matriz B: ";
    for(int i=0;i<MRnumB;i++){
            for(int j=0;j<MRnumB;j++){
                cin>>MRMatrizB[i][j];
            }
    }
    for(int i=0;i<MRnumA;i++){
            for(int j=0;j<MRnumB;j++){
                MRsumadeAB[i][j] = MRMatrizA[i][j] + MRMatrizB[i][j];
            }
    }
    for(int i=0;i<MRnumA;i++){
            for(int j=0;j<MRnumB;j++){
                cout<<MRsumadeAB[i][j]<<" ";
            }
            cout<<endl;
    }
    
    return 0;
}
